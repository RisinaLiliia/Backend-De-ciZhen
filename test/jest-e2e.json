{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.json"
      }
    ]
  },
  "setupFiles": ["<rootDir>/jest.e2e-setup.js"],
  "clearMocks": true,
    "forceExit": true,
  "detectOpenHandles": true
}
