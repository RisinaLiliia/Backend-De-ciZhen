            }
          }
        },
        "tags": [
          "auth"
        ]
      }
    }
  },
  "info": {
    "title": "Decide API",
    "description": "API documentation",
    "version": "1.0.0",
    "contact": {},
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development"
    }
  ],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "HttpErrorDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 401
          },
          "message": {
            "oneOf": [
              {
                "type": "string",
                "example": "Unauthorized"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "example": [
                  "email must be an email"
                ]
              }
            ]
          },
          "error": {
            "type": "string",
            "example": "Unauthorized"
          },
          "timestamp": {
            "type": "string",
            "example": "2026-01-27T10:20:30.123Z"
          },
          "path": {
            "type": "string",
            "example": "/auth/login"
          },
          "requestId": {
            "type": "string",
            "example": "a9f7c8f0-1b2c-4d5e-9f00-123456789abc",
            "description": "Request correlation id (x-request-id)"
          },
          "stack": {
            "type": "string",
            "example": "stack trace...",
            "description": "Only in development"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error",
          "timestamp",
          "path"
        ]
      },
      "AvatarDto": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "example": "/avatars/default.png"
          },
          "isDefault": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "url",
          "isDefault"
        ]
      },
      "MeResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "64f0c1a2b3c4d5e6f7a8b9c0"
          },
          "name": {
            "type": "string",
            "example": "Liliia"
          },
          "email": {
            "type": "string",
            "example": "liliia@test.com"
          },
          "role": {
            "type": "string",
            "enum": [
              "client",
              "provider",
              "admin"
            ],
            "example": "client"
          },
          "city": {
            "type": "string",
            "example": "Berlin"
          },
          "language": {
            "type": "string",
            "example": "de"
          },
          "phone": {
            "type": "string",
            "example": "+49123456789"
          },
          "avatar": {
            "$ref": "#/components/schemas/AvatarDto"
          },
          "acceptedPrivacyPolicy": {
            "type": "boolean",
            "example": true
          },
          "acceptedPrivacyPolicyAt": {
            "type": "object",
            "example": "2025-01-01T12:00:00.000Z"
          },
          "isBlocked": {
            "type": "boolean",
            "example": false
          },
          "blockedAt": {
            "type": "object",
            "example": null
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-01-01T12:00:00.000Z"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-01-02T12:00:00.000Z"
          }
        },
        "required": [
          "id",
          "name",
          "email",
          "role",
          "acceptedPrivacyPolicy",
          "isBlocked",
          "createdAt",
          "updatedAt"
        ]
      },
      "UpdateMeDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Liliia"
          },
          "city": {
            "type": "string",
            "example": "Berlin"
          },
          "language": {
            "type": "string",
            "example": "de"
          },
          "phone": {
            "type": "string",
            "example": "+49123456789"
          },
          "avatarUrl": {
            "type": "string",
            "example": "https://cdn.example.com/u/1.png"
          }
        }
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Liliia",
            "description": "Full name"
          },
          "email": {
            "type": "string",
            "example": "liliia@example.com",
            "description": "Email"
          },
          "password": {
            "type": "string",
            "example": "Password1",
            "description": "Password"
          },
          "city": {
            "type": "string",
            "example": "Berlin",
            "description": "City (optional, can be set later)"
          },
          "language": {
            "type": "string",
            "example": "de",
            "description": "Language code (optional)"
          },
          "role": {
            "type": "string",
            "example": "client",
            "description": "Role: client or provider (admin only via backoffice)"
          },
          "acceptPrivacyPolicy": {
            "type": "boolean",
            "example": true,
            "description": "Accept privacy policy"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "acceptPrivacyPolicy"
        ]
      },
      "SafeUserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "64f0c1a2b3c4d5e6f7a8b9c0"
          },
          "name": {
            "type": "string",
            "example": "Liliia"
          },
          "email": {
            "type": "string",
            "example": "liliia@test.com"
          },
          "role": {
            "type": "string",
            "enum": [
              "client",
              "provider",
              "admin"
            ],
            "example": "client"
          },
          "city": {
            "type": "string",
            "example": "Berlin"
          },
          "language": {
            "type": "string",
            "example": "de"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-01-01T12:00:00.000Z"
          }
        },
        "required": [
          "id",
          "name",
          "email",
          "role"
        ]
      },
      "AuthResponseDto": {
        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/SafeUserDto"
          },
          "accessToken": {
            "type": "string",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "expiresIn": {
            "type": "number",
            "example": 900,
            "description": "Access token TTL in seconds"
          }
        },
        "required": [
          "user",
          "accessToken",
          "expiresIn"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "Password1"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RefreshResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "expiresIn": {
            "type": "number",
            "example": 900,
            "description": "Access token TTL in seconds"
          }
        },
        "required": [
          "accessToken",
          "expiresIn"
        ]
      },
      "LogoutResponseDto": {
        "type": "object",
        "properties": {
          "ok": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "ok"
        ]
      }
    }
  }
}